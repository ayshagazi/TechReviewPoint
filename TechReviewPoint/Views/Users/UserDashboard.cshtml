@model TechReviewPoint.Models.User

@{
    Layout = null;
    if (Session["UserSessionEmail"] == null)
    {
        Response.Redirect("~/Users/Login");
    }
}

<h2>UserDashboard</h2>

<div>
   
    <h1>User Email: @Session["UserSessionEmail"]</h1>
    <a href="@Url.Action("UserLogout","Users")">Log Out</a>

</div>


